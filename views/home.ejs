          <h5 class="card-title">Register a new student</h5>
          <a class="card-link" href="/student-registration">Register</a>
        </div>
      </div>
    </div>
    <div class="col">
      <div class="card">
        <img src="/images/search.svg" height="200px" class="card-img-top" alt="Search">
        <div class="card-body">
          <h5 class="card-title">Search student record by email</h5>
          <a class="card-link" href="/search">Search</a>
        </div>
      </div>
    </div>
  </div>

<!-- end of links -->

 </div>
   
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js">
